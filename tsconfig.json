{
	"compilerOptions": {
	  "sourceMap": true,
	  "target": "ES6",
	  "module": "ESnext",
	  "outDir": "public",
	  "jsx": "react-jsx",
	  "jsxImportSource": "@emotion/react",
	  "moduleResolution": "Node",
	  "lib": ["ES2020", "DOM"],
	  "allowJs": true,
	  "allowSyntheticDefaultImports": true,
	  "strict": true
	},
	"include": ["src/**/*"],
	//"exclude": ["./node_modules"],
}